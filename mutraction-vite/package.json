  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.23.3",
    "esbuild": "^0.19.1",
    "typescript": "^5.1.6",
    "vite": "^5.0.11",
    "vite-plugin-babel": "^1.2.0"
  },
  "dependencies": {
    "mutraction-dom": "x"
  }
}
