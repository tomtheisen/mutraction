  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.18.16",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@types/react": ">=18.0.0",
    "@types/react-dom": ">=18.0.0",
    "mutraction-react": "^0.13.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
