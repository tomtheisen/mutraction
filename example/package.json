    "bundle:binding": "npx esbuild --bundle react-binding/binding.js --outfile=react-binding/binding_bundle.js",
    "bundle:todo": "npx esbuild --bundle react-todo/todo.js --outfile=react-todo/todo_bundle.js"
  },
