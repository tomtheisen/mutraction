{
  "name": "mutraction-react",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomtheisen/mutraction.git"
  },
  "version": "0.13.0",
  "description": "React integration for mutraction.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist/*.*", "dist/src"],
  "scripts": {
    "build": "npm run clean && npx tsc && npm run bundle && npm run types && npm run cleanout",
    "bundle": "npx esbuild .\\out\\index.js --bundle --format=esm --outfile=.\\dist\\index.js --external:react --external:mutraction",
    "types": "npx tsc -p tsconfig.types.json",
    "clean": "del /Q /S dist && del /Q /S out",
    "cleanout": "rd /Q /S out"
  },
  "author": "Tom Theisen",
  "license": "MIT",
  "devDependencies": {
    "@types/react": ">=18.0.0",
    "@types/react-dom": ">=18.0.0",
    "esbuild": "^0.18.16",
    "typescript": "^5.1.6",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "mutraction": "^0.13.0"
  }
}
