{
  "name": "mutraction-react",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomtheisen/mutraction"
  },
  "version": "0.9.0",
  "description": "React integration for mutraction.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist/*.*", "dist/src"],
  "scripts": {
    "bundle": "npm run clean && npx tsc && npm run combine && npm run types",
    "combine": "npx esbuild .\\out\\index.js --bundle --format=esm --outfile=.\\dist\\index.js --external:react --external:mutraction",
    "types": "npx tsc --declaration --emitDeclarationOnly --module ES2022 --target ES2022 --outDir dist --jsx react --moduleResolution Node16 .\\index.ts",
    "test": "npx tsc && npx uvu dist/tests -i \"[.]d[.]ts$\"",
    "clean": "del /Q /S dist && del /Q /S out"
  },
  "author": "Tom Theisen",
  "license": "MIT",
  "devDependencies": {
    "@types/react": ">=18.0.0",
    "@types/react-dom": ">=18.0.0",
    "esbuild": "^0.18.15",
    "typescript": "^5.1.6",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "mutraction": "^0.8.0"
  }
}
