{
  "name": "mutraction-dom",
  "type": "module",
  "version": "0.13.0",
  "description": "DOM synchronization for mutable models ",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomtheisen/mutraction.git"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/*.*",
    "dist/src"
  ],
  "scripts": {
    "transform": "npx tsc && ..\\node_modules\\.bin\\babel .\\src.js",
    "build": "npm run clean && npx tsc && npm run bundle && npm run types && npm run cleanout",
    "bundle": "npx esbuild .\\out\\index.jsx --bundle --outfile=.\\dist\\index.js",
    "types": "",
    "_types": "npx tsc -p tsconfig.types.json",
    "test": "npx tsc && npx uvu out/tests -i \"[.]d[.]ts$\"",
    "clean": "del /Q /S dist && del /Q /S out",
    "cleanout": "rd /Q /S out"
  },
  "author": "Tom Theisen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomtheisen/mutraction/issues"
  },
  "homepage": "https://github.com/tomtheisen/mutraction#readme",
  "dependencies": {
    "mutraction": "^0.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/plugin-syntax-jsx": "^7.22.5",
    "@babel/preset-env": "^7.22.10",
    "@types/babel__core": "^7.20.1"
  }
}
